FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue zoneID 123
);

regions
(
    // Set cell values
    // (does zerogradient on boundaries)
    cellToCell
    {
        set Background-cells;

        fieldValues
        (
            volScalarFieldValue zoneID 0
        );
    }

    cellToCell
    {
        set Wing1-cells;

        fieldValues
        (
            volScalarFieldValue zoneID 1
        );
    }
    
    cellToCell
    {
        set Wing2-cells;

        fieldValues
        (
            volScalarFieldValue zoneID 2
        );
    }

);

