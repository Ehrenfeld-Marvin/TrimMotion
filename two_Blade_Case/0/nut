FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"


    Inlet
    {
        type            calculated;
        value           uniform 0;
    }
    Outlet
    {
        type            calculated;
        value           uniform 0;
    }
    Border
    {
        type            calculated;
        value           uniform 0;
    }
    "(Overset1|Overset2|)"
    {
        type            overset;
        
    }
    BaseAndTop
    {
        type            empty;
    }

    "(Hole1|Hole2)"
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
}

