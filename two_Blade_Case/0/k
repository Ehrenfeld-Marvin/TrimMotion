FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

#include        "include/initialConditions"

internalField   $turbulentKE;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(Overset1|Overset2)"
    {
        type            overset;
    }
    BaseAndTop
    {
        type            empty;
    }
    Border
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            fixedValue;
        value		 $internalField;
    }
    
    Outlet
    {
        type            zeroGradient;
        
    }

    "(Hole1|Hole2)"
    {
        type            kLowReWallFunction;
        value		 $internalField;
    }
}

