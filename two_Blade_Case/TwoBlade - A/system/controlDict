FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs
(
 "libTrimMotions.so"
);


DebugSwitches
{
    overset                 0; 	//0 off - 1 give more information
    dynamicOversetFvMesh    0;
    cellVolumeWeight        0;
    inverseDistance	    0;	//2 Force dumping of cellInterpolationWeight
}

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt endTime;

endTime 		20;
deltaT  		0.00005;
writeControl  		adjustableRunTime;

writeInterval   	20; // 1.5;
adjustTimeStep  	yes;
maxCo       		40;
maxDeltaT   		0.0018697; 

purgeWrite      0;

writeFormat     binary;

writePrecision  16;

writeCompression off;

timeFormat      general;

timePrecision   9;

runTimeModifiable true;



functions
{
#include "forces.allBlades"
}

